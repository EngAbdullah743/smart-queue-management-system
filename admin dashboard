<script>
    // Staff Edit Modal Functionality
setTimeout(function() {
    // Staff data in array format (matching the order in the table)
    const staffData = [
        {
            name: "John Smith",
            email: "john@cityhairstudio.com",
            role: "Staff",
            business: "City Salon",
            phone: "(555) 234-5678",
            password: "Js123456"
        },
        // Additional staff members...
    ];
    
    // Add click handlers to edit buttons
    const staffTab = document.getElementById('staff-tab');
    if (staffTab) {
        const editButtons = staffTab.querySelectorAll('.action-btn.edit');
        
        editButtons.forEach((button, index) => {
            button.addEventListener('click', function(e) {
                e.preventDefault();
                
                // Get staff data and populate form
                if (index < staffData.length) {
                    const staff = staffData[index];
                    
                    document.getElementById('edit-staff-name').textContent = staff.name;
                    document.getElementById('edit-staff-index').value = index;
                    document.getElementById('edit-staff-email').value = staff.email;
                    document.getElementById('edit-staff-role').value = staff.role;
                    document.getElementById('edit-staff-business').value = staff.business;
                    document.getElementById('edit-staff-phone').value = staff.phone;
                    
                    document.getElementById('staff-edit-modal').style.display = 'block';
                }
            });
        });
        
        // Save button handler
        // Close button handlers
        // etc...
    }
}, 1500);
    </script>

   <!-- Staff Edit Modal -->
